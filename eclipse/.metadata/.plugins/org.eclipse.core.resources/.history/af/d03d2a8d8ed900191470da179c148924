package packq3;
import java.util.Scanner;
public class q3 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("enter the no. of inputs");
		int no = sc.nextInt();
		Student[] s = new Student[no];
		
		for(int i = 0; i < no; i++)
		{	
			s[i].name = sc.nextLine();
			s[i].year = sc.nextInt();
		}
	}
}

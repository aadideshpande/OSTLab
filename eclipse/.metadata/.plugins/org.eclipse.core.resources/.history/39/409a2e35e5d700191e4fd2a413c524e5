
public class four {
	
	static void demo() {
		try {
			throw new NullPtrExc("demo");
		}
		catch(NullPtrExc e){
			System.out.println("we are inside the catch ");
			throw e;
		}
	}
	
	public static void main(String[] args) {
		try {
			demo();
			catch(NullPtrExc e) {
				System.out.println("recaught: " + e);
			}
		}
	}
}
